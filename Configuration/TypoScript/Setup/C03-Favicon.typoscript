page {
  # Generete favicon with https://realfavicongenerator.net/ and put the generated files into webroot (.build/public or public)
  shortcutIcon = EXT:iucunda/Resources/Public/Icons/Favicon/favicon.ico
  headerData.100 = TEXT
  headerData.100.value (

    <link rel="icon" sizes="48x48" type="image/png" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/favicon-48x48.png}"/>
    <link rel="icon" type="image/svg+xml" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/favicon.svg}" />
    <link rel="shortcut icon" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/favicon.ico}" />
    <link rel="apple-touch-icon" sizes="180x180" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/apple-touch-icon.png}" />
    <meta name="apple-mobile-web-app-title" content="Iucunda" />
    <link rel="manifest" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/site.webmanifest}" />
    <link rel="icon" sizes="192x192" type="image/png" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/web-app-manifest-192x192.png}"/>
    <link rel="icon" sizes="512x512" type="image/png" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon/web-app-manifest-512x512.png}"/>
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{path : EXT:iucunda/Resources/Public/Icons/Favicon/web-app-manifest-192x192.png}">
  )
  headerData.100.insertData = 1
  # <link rel="icon" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon}/white-logo.png" media="(prefers-color-scheme: dark)"/>
  # <link rel="icon" href="{path : EXT:iucunda/Resources/Public/Icons/Favicon}/black-logo.png" media="(prefers-color-scheme: light)"/>
}
